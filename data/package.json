    "files": [
        "dist",
        "public"
    ],
    "scripts": {
        "dev": "vite build --watch",
        "build": "vite build",
        "build:types": "tsc --emitDeclarationOnly"
    },
    "dependencies": {
        "prop-types": "latest"
    },
    "devDependencies": {
        "typescript": "^5.2.2",
        "vite": "^5.0.0"
    },
    "keywords": [
        "micetf",
        "data"
    ],
    "author": "Frédéric Misery <webmaster@micetf.fr>",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
